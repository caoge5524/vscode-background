{
  "name": "vscode-background",
  "displayName": "%ext.displayName%",
  "description": "%ext.description%",
  "version": "2.0.2",
  "publisher": "caoge",
  "author": {
    "name": "操歌"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caoge5524/vscode-background.git"
  },
  "bugs": {
    "url": "https://github.com/caoge5524/vscode-background/issues"
  },
  "homepage": "https://github.com/caoge5524/vscode-background#readme",
  "keywords": [
    "background",
    "video",
    "mp4",
    "webm",
    "theme",
    "customization",
    "wallpaper",
    "transparent"
  ],
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-background.install",
        "title": "%cmd.install%"
      },
      {
        "command": "vscode-background.uninstall",
        "title": "%cmd.uninstall%"
      },
      {
        "command": "vscode-background.addVideos",
        "title": "%cmd.addVideos%"
      },
      {
        "command": "vscode-background.diagnostics",
        "title": "%cmd.diagnostics%"
      }
    ],
    "configuration": {
      "title": "%config.title%",
      "properties": {
        "vscodeBackground.enabled": {
          "type": "boolean",
          "default": false,
          "scope": "application",
          "markdownDescription": "%config.enabled%",
          "order": 1
        },
        "vscodeBackground.videos": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "scope": "application",
          "markdownDescription": "%config.videos%",
          "order": 2
        },
        "vscodeBackground.opacity": {
          "type": "number",
          "default": 0.8,
          "minimum": 0,
          "maximum": 1,
          "scope": "application",
          "markdownDescription": "%config.opacity%",
          "order": 3
        },
        "vscodeBackground.switchInterval": {
          "type": "number",
          "default": 180,
          "minimum": 0,
          "scope": "application",
          "markdownDescription": "%config.switchInterval%",
          "order": 4
        },
        "vscodeBackground.theme": {
          "type": "string",
          "default": "glass",
          "enum": [
            "glass",
            "matte",
            "neon",
            "cinema",
            "aurora",
            "minimal",
            "retro"
          ],
          "enumDescriptions": [
            "%config.theme.glass%",
            "%config.theme.matte%",
            "%config.theme.neon%",
            "%config.theme.cinema%",
            "%config.theme.aurora%",
            "%config.theme.minimal%",
            "%config.theme.retro%"
          ],
          "scope": "application",
          "markdownDescription": "%config.theme%",
          "order": 5
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "vscode:uninstall": "node ./out/uninstall.js",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}